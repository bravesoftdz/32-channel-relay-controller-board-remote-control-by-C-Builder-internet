if (OpenDialog1->Execute())
 {
   Image59->Picture->LoadFromFile(OpenDialog1->FileName);

   ini = new TIniFile(ChangeFileExt( Application->ExeName, ".INI" ) );
   ini->WriteString ( "Device_ICO", "Light30_OFF", OpenDialog1->FileName );
   delete ini;
 }
}
